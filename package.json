{
  "name": "open-sourcetree-in-vscode-plugin",
  "version": "1.0.3",
  "description": "WIP - nothing to see here",
  "main": "index.js",
  "engines": {
    "vscode": "^1.32.0"
  },
  "publisher": "liangcheng-plugins",
  "activationEvents": [
    "onCommand:extension.sourcetree"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.sourcetree",
        "title": "open in sourcetree"
      }
    ],
    "menus": {
      "notebook/toolbar": [
        {
          "command": "extension.sourcetree",
          "group": "navigation"
        }
      ],
      "scm/title": [
        {
          "command": "extension.sourcetree",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "extension.sourcetree",
          "group": "1_diff"
        }
      ],
      "file/newFile": [
        {
          "command": "extension.sourcetree",
          "group": "navigation"
        }
      ],
      "explorer/context": [
        {
          "command": "extension.sourcetree",
          "group": "1_diff"
        }
      ],
      "commandPalette": [
        {
          "command": "extension.sourcetree"
        }
      ]
    }
  },
  "scripts": {
    "prepare": "echo prepare && npm run build",
    "prebuild": "npm run clean ",
    "clean": "rimraf dist/*",
    "build": " echo do something for build",
    "pretest": "echo pretest....",
    "test": "run-s test:*",
    "test:deps": "dependency-check . --no-dev --no-peer",
    "test:standard": "standard",
    "version": "run-s  version:*",
    "version:changelog": "auto-changelog -p --template keepachangelog auto-changelog --breaking-pattern 'BREAKING CHANGE:'",
    "version:git": "git add .",
    "prepublishOnly": "git push --follow-tags && gh-release -y"
  },
  "devDependencies": {
    "auto-changelog": "^2.3.0",
    "dependency-check": "^4.1.0",
    "gh-release": "^6.0.1",
    "npm-run-all2": "^5.0.2",
    "ovsx": "^0.2.1",
    "rimraf": "^3.0.2",
    "standard": "^16.0.4",
    "vscode": "^1.1.37"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/slcnx/open-sourcetree-in-vscode-plugin.git"
  },
  "author": "Songliangcheng (@slcnx)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/slcnx/open-sourcetree-in-vscode-plugin/issues"
  },
  "homepage": "https://github.com/slcnx/open-sourcetree-in-vscode-plugin",
  "dependencies": {
    "open": "^8.4.0"
  }
}
